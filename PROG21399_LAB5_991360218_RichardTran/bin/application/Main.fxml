<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Text layoutX="260.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Age:" />
      <TextField fx:id="TxtFieldAge" layoutX="260.0" layoutY="117.0" prefHeight="25.0" prefWidth="37.0" />
      <Button fx:id="BtnDisplayFee" layoutX="253.0" layoutY="260.0" mnemonicParsing="false" onAction="#calculate" text="Display Fee" />
      <Button fx:id="BtnExit" layoutX="253.0" layoutY="302.0" mnemonicParsing="false" onAction="#Exit" prefHeight="25.0" prefWidth="76.0" text="Exit" />
      <Text layoutX="51.0" layoutY="277.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Fee:" />
      <TextField fx:id="TxtFieldFee" editable="false" layoutX="51.0" layoutY="302.0" prefHeight="25.0" prefWidth="110.0" />
      <RadioButton fx:id="RdBtnMemeber" layoutX="51.0" layoutY="88.0" mnemonicParsing="false" selected="true" text="Member">
         <toggleGroup>
            <ToggleGroup fx:id="membertogglegroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="RdBtnNonMemeber" layoutX="51.0" layoutY="134.0" mnemonicParsing="false" text="Non-Member" toggleGroup="$membertogglegroup" />
   </children>
</AnchorPane>
