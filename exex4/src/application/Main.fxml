<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <TextArea fx:id="txtArea" layoutX="214.0" layoutY="14.0" prefHeight="120.0" prefWidth="172.0" promptText="Enter a Number" />
      <Button fx:id="btnStart" layoutX="66.0" layoutY="315.0" mnemonicParsing="false" text="Start" />
      <Button fx:id="btnClear" layoutX="262.0" layoutY="315.0" mnemonicParsing="false" text="Clear" />
      <Button fx:id="btnParse" layoutX="148.0" layoutY="315.0" mnemonicParsing="false" onAction="#onParseClicked" text="Parse" />
      <TextField fx:id="txtuserinput" layoutX="25.0" layoutY="14.0" prefHeight="120.0" prefWidth="165.0" />
   </children>
</AnchorPane>
